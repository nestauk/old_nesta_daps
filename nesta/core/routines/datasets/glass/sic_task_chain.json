[
    {
	"batchable": "batchables/nlp/ngrammer",
	"env_files": [
	    "/nesta",
	    "/config/mysqldb.config"
	],
        "job_def": "py36_sklearn_amzn1_image",
	"job_name": "ngram",
	"job_queue": "HighPriority",
	"n_batches": 100,
	"memory": 4000,
	"poll_time": 20,
	"region_name": "eu-west-2",
	"use_intermediate_inputs": false
    },
    {
	"batchable": "batchables/nlp/vectorizer",
	"hyperparameters": {
	    "binary": [true, false],
	    "min_df": 0.001,
	    "max_df": 0.99
	},
	"job_name": "vectorizer",
	"memory": 16000,
	"n_batches": 1,
	"timeout": 72000
    },
    {
	"job_name": "logistic_regression_liblinearL1L2",
	"batchable": "batchables/sklearn_models/multilabel_classifier",
	"hyperparameters": {
	    "model": ["linear_model.LogisticRegression"],
	    "max_iter": 50000,
	    "solver": ["liblinear"],
	    "penalty": ["l1", "l2"],
	    "class_weight": ["balanced", null],
	    "C": "np.arange(0.1, 1.0, 0.2)",
	    "intercept_scaling": "np.arange(0.2, 2.2, 0.4)",
	    "n_jobs": 2,
	    "class_field": ["sic"]
	},
	"combine_outputs": false
    },
    {
	"job_name": "logistic_regression_liblinearL2",
	"child": "vectorizer",
	"batchable": "batchables/sklearn_models/multilabel_classifier",
	"hyperparameters": {
	    "model": ["linear_model.LogisticRegression"],
	    "max_iter": 50000,
	    "solver": ["liblinear"],
	    "penalty": ["l2"],
	    "dual": true,
	    "class_weight": ["balanced", null],
	    "C": "np.arange(0.1, 1.0, 0.2)",
	    "intercept_scaling": "np.arange(0.2, 2.2, 0.4)",
	    "n_jobs": 2,
	    "class_field": ["sic"]
	},
	"combine_outputs": false
    },
    {
	"job_name": "logistic_regression_sagaOther",
	"child": "vectorizer",
	"batchable": "batchables/sklearn_models/multilabel_classifier",
	"hyperparameters": {
	    "model": ["linear_model.LogisticRegression"],
	    "max_iter": 50000,
	    "solver": ["saga"],
	    "penalty": ["l1", "l2", "none"],
	    "class_weight": ["balanced", null],
	    "C": "np.arange(0.1, 1.0, 0.4)",
	    "n_jobs": 2,
	    "class_field": ["sic"]
	},
	"combine_outputs": false
    },
    {
	"job_name": "logistic_regression_sagaENetl1",
	"child": "vectorizer",
	"batchable": "batchables/sklearn_models/multilabel_classifier",
	"hyperparameters": {
	    "model": ["linear_model.LogisticRegression"],
	    "max_iter": 50000,
	    "solver": ["saga"],
	    "penalty": ["elasticnet"],
	    "class_weight": ["balanced", null],
	    "C": "np.arange(0.1, 1.0, 0.2)",
	    "l1_ratio": "np.arange(0.0, 1.1, 0.4)",
	    "n_jobs": 2,
	    "class_field": ["sic"]
	},
	"combine_outputs": false
    },
    {
	"job_name": "logistic_regression_otherSolver",
	"child": "vectorizer",
	"batchable": "batchables/sklearn_models/multilabel_classifier",
	"hyperparameters": {
	    "model": ["linear_model.LogisticRegression"],
	    "max_iter": 50000,
	    "solver": ["newton-cg", "lbfgs", "sag"],
	    "penalty": ["l2", "none"],
	    "class_weight": ["balanced", null],
	    "C": "np.arange(0.1, 1.0, 0.4)",
	    "n_jobs": 2,
	    "class_field": ["sic"]
	},
	"combine_outputs": false
    }
]
